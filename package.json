{
  "name": "vanilla-basic",
  "version": "1.0.0",
  "scripts": {
    "start": "sirv ./src public --cors --single --no-clear --port 8000",
    "test": "jest --coverage",
    "badge": "npm test && jest-coverage-badges"
  },
  "dependencies": {
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^1.1.7",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "jsdom",
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  }
}
